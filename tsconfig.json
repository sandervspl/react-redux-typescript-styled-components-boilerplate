{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "removeComments": true,
    "allowUnreachableCode": true,
    "pretty": true,
    "module": "commonjs",
    "target": "es2015",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "common": ["src/app/components/common/index"],
      "common/*": ["src/app/components/common/*"],
      "components/*": ["src/app/components/*"],
      "ducks/*": ["src/app/ducks/*"],
      "images/*": ["src/app/static/images/*"],
      "modules": ["src/app/components/modules/index"],
      "modules/*": ["src/app/components/modules/*"],
      "server/*": ["src/server"],
      "services": ["src/app/services/index"],
      "services/*": ["src/app/services/*"],
      "styles": ["src/app/styles/index"],
      "styles/*": ["src/app/styles/*"],
      "types": ["src/app/types/index"],
      "vectors/*": ["src/app/static/vectors/*"],
      "styled-components": ["src/app/services/styled-components.ts"],
      "app/*": ["src/app/*"]
    },
    "typeRoots": ["node_modules/@types"],
    "lib": ["dom", "es6"]
  }
}
